{
    "name": "tinywan/composer-package-template",
    "description": "composer-package-template",
    "type": "library",
    "license": "Apache License",
    "authors": [
        {
            "name": "Tinywan",
            "email": "756684177@qq.com"
        }
    ],
    "require": {
        "php": ">=7.1",
        "ext-json": "*",
        "yansongda/supports": "~3.1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "mockery/mockery": "^1.4",
        "friendsofphp/php-cs-fixer": "^3.0",
        "monolog/monolog": "^2.2",
        "symfony/var-dumper": "^5.1",
        "symfony/http-foundation": "^5.2.0",
        "symfony/event-dispatcher": "^5.2.0",
        "symfony/psr-http-message-bridge": "^2.1"
    },
    "autoload": {
        "psr-4": {
            "Tinywan\\Template\\": "src"
        },
        "files": [
            "src/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tinywan\\Template\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "./vendor/bin/phpunit -c phpunit.xml --colors=always",
        "phpcs": "vendor/bin/php-cs-fixer fix"
    },
    "repositories": {
        "packagist": {
            "type": "composer",
            "url": "https://mirrors.aliyun.com/composer/"
        }
    }
}